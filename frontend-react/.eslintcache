1) {\n            if (currentOrders[orderIndex].quantity > 0) {\n                currentOrders.splice(orderIndex, 1, {\n                    \"hotdog_id\": event.target.id,\n                    \"quantity\": currentOrders[orderIndex].quantity - 1,\n                    \"vendor_id\": this.props.vendorId\n                })\n            }\n            this.setState({orderedItems: currentOrders})\n        }\n    }\n\n    numberOrdered = (id) => {\n        const found = this.state.orderedItems.find(ele => ele.hotdog_id == id);\n        return found ? found.quantity : 0;\n    }\n\n    onSubmit = () => {\n        console.log(JSON.stringify(this.state.orderedItems))\n        fetch(\"/orders\" , {\n            method: \"POST\",\n            headers: {\n                'Content-type': 'application/json'\n            },\n            body: JSON.stringify(this.state.orderedItems)\n        }).then(result => result.json()).then(json => console.log(json))\n}\n\n\n    render() {\n        return (\n            <div>\n                <CardDeck>\n                    {this.state.menuItems.map(menuItem =>\n                        <div key={menuItem.hotdog_id}>\n                            <Card>\n                                <Card.Img\n                                    style={{width: \"18rem\"}}\n                                    variant=\"top\"\n                                    src=\"https://i.pinimg.com/originals/5d/c6/38/5dc63889a0b76b9a0ce4bc1eb291ae00.png\">\n                                </Card.Img>\n                                <Card.Body id={menuItem.hotdog_id}>\n                                    <Card.Title>{menuItem.hotdog_name}</Card.Title>\n                                    {this.numberOrdered(menuItem.hotdog_id) !== 0 &&\n                                    <Card.Text>number in cart: {this.numberOrdered(menuItem.hotdog_id)}</Card.Text>\n                                    }\n                                    <Button variant=\"outline-dark\" onClick={this.handleIncrement}\n                                            id={menuItem.hotdog_id}>\n                                        +\n                                    </Button>\n                                    <Button variant=\"outline-dark\" onClick={this.handleDecrement}\n                                            id={menuItem.hotdog_id}>\n                                        -\n                                    </Button>\n                                </Card.Body>\n                            </Card>\n                        </div>\n                    )}\n                </CardDeck>\n                <Button variant=\"primary\" onClick={this.onSubmit}> does thing </Button>\n            </div>\n        );\n    }\n}\n\nexport default Menu;\n","/Users/daleberg/AD-320Folder/ad320-thurs-w21-team1/frontend-react/src/Contact.js",["40"],"import React, { Component } from 'react';\n\n\nclass Contact extends React.Component {\n\n    render() {\n        return (\n            <div>\n                <p>This is contact</p>\n                <ul>\n                    <li>Some stuff</li>\n                    <li>Some stuff</li>\n                    <li>Some stuff</li>\n                </ul>\n            </div>\n        )\n    }\n}\n\nexport default Contact;\n","/Users/daleberg/AD-320Folder/ad320-thurs-w21-team1/frontend-react/src/Home.js",["41"],"import React, { Component } from 'react';\n\nclass Home extends React.Component {\n    constructor(props) {\n        super(props);\n        this.state = { apiResponse: \"\" };\n    }\n\n    callBackend() {\n        console.log(\"api called\");\n        fetch(\"/users\")\n            .then(res => res.text())\n            .then(res => this.setState({ apiResponse: res }))\n            .catch(err => console.log(err));\n    }\n\n    componentWillMount() {\n        this.callBackend();\n    }\n\n    render() {\n        return (\n            <div>\n                <h2>Hey, you're home!</h2>\n                <p>This is a bunch of text!</p>\n                <p>This a bunch more text!</p>\n            </div>\n        );\n    }\n}\n\nexport default Home;\n","/Users/daleberg/AD-320Folder/ad320-thurs-w21-team1/frontend-react/src/MapContainer.jsx",["42"],"/Users/daleberg/AD-320Folder/ad320-thurs-w21-team1/frontend-react/src/Admin.js",["43"],"import React, { Component } from 'react';\n\n\nclass Admin extends React.Component {\n\n    render() {\n        return (\n            \n     \n            <form>\n                <label>\n                <center>New Vendor Account<br /><br /></center>\n                Vendor Name: <input type = \"text\" />\n                Vendor Username: <input type = \"text\" /><br /><br />\n                Vendor Role: <input type = \"text\" />\n                Vendor Password: <input type = \"password\" /><br /><br />\n                <input type = \"button\" value = \"Submit\" />\n                </label>\n            </form>\n        )\n    }\n}\n      \n  \nexport default Admin;\n",{"ruleId":"44","replacedBy":"45"},{"ruleId":"46","replacedBy":"47"},{"ruleId":"48","severity":1,"message":"49","line":1,"column":16,"nodeType":"50","messageId":"51","endLine":1,"endColumn":25},{"ruleId":"48","severity":1,"message":"49","line":1,"column":16,"nodeType":"50","messageId":"51","endLine":1,"endColumn":25},{"ruleId":"52","severity":1,"message":"53","line":58,"column":73,"nodeType":"54","messageId":"55","endLine":58,"endColumn":75},{"ruleId":"48","severity":1,"message":"49","line":1,"column":17,"nodeType":"50","messageId":"51","endLine":1,"endColumn":26},{"ruleId":"48","severity":1,"message":"49","line":1,"column":17,"nodeType":"50","messageId":"51","endLine":1,"endColumn":26},{"ruleId":"48","severity":1,"message":"49","line":1,"column":16,"nodeType":"50","messageId":"51","endLine":1,"endColumn":25},{"ruleId":"48","severity":1,"message":"49","line":1,"column":17,"nodeType":"50","messageId":"51","endLine":1,"endColumn":26},"no-native-reassign",["56"],"no-negated-in-lhs",["57"],"no-unused